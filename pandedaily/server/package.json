{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon server.js",
    "sequelize:init": "sequelize init",
    "db:setup": "node src/database/utility/dbSetup.util.js",
    "db:status": "node src/database/utility/checkMigrations.util.js",
    "db:migrate": "npx sequelize-cli db:migrate && node src/database/utility/generateModels.util.js",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo",
    "db:seed:undo": "npx sequelize-cli db:seed:undo:all",
    "db:drop": "node src/database/settings/confirmation.settings.js \"⚠️ WARNING: This will DROP the database. Type 'yes' to continue: \" && npx sequelize-cli db:drop",
    "db:create": "npx sequelize-cli db:create",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:reset": "node src/database/settings/confirmation.settings.js \"⚠️ WARNING: This will RESET the database. Type 'yes' to continue: \" && npm run db:drop && npm run db:create && npm run db:migrate && npm run db:seed",
    "env:setup": "node src/database/settings/envSetup.settings.js",
    "generate:schema": "node src/database/utility/generateSchema.util.js",
    "db:migrate:prod": "npx sequelize-cli db:migrate --migrations-path database/migrations/alter && node database/utility/generateModels.util.js",
    "db:migrate:prod:undo": "npx sequelize-cli db:migrate:undo --migrations-path database/migrations/alter",
    "encrypt": "node src/utils/cryptography.util.js encrypt",
    "decrypt": "node src/utils/cryptography.util.js decrypt"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "body-parser": "^2.2.2",
    "connect-mongo": "^6.0.0",
    "connect-mongodb-session": "^5.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.6",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-session": "^1.19.0",
    "fs-extra": "^11.3.3",
    "joi": "^18.0.2",
    "jsonwebtoken": "^9.0.3",
    "mongoose": "^9.1.5",
    "morgan": "^1.10.1",
    "mysql2": "^3.16.2",
    "sequelize": "^6.37.7",
    "sequelize-pack": "^1.0.4",
    "swagger": "^0.7.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "umzug": "^3.8.2",
    "winston": "^3.19.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.11",
    "sequelize-cli": "^6.6.5"
  }
}
